<bind tag="LaTeXFragment:inline">
  <span class="math inline">\(<FragmentContents />\)</span>
</bind>

<bind tag="LaTeXFragment:display">
  <span class="math display">\[<FragmentContents />\]</span>
</bind>

<!-- people may prefer this to be empty? -->
<bind tag="LaTeXFragment:raw">
  <span class="latex raw">\(<FragmentContents />\)</span>
</bind>

