<section id="${anchor}">
  <headline>
    <case todo>
      <span class="todokw todo">
        <todo-name/>
      </span>
    </case>
    <case done>
      <span class="todokw done">
        <todo-name/>
      </span>
    </case>
    <priority>
      <span class="priority ${value}">[#<value/>]</span>
    </priority>
    <headline-title/>
    <tags>
      <span class="tag tag-${tag}"><tag/></span>
    </tags>
  </headline>
  <contents/>
</section>
