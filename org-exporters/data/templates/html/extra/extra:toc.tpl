<div id="table-of-contents">
  <h2>Table of Contents</h2>
  <scope>
    <bind org:sections>
      <ul>
        <sections>
          <li>
            <a href="#!(anchor)"><section:headline /></a>
            <section:subsections />
          </li>
        </sections>
      </ul>
    </bind>
    <doc:sections />
  </scope>
</div>
